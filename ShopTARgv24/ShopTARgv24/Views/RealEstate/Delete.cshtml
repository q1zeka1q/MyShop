@model ShopTARgv24.Models.RealEstates.RealEstateDeleteViewModel

<h1>Delete</h1>

<div>
    <h4>Spaceship</h4>
    <hr />
    <dl class="row">
        <div class="form-group row"> 
            <label asp-for="Id" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(Model => Model.Id)
        </div>
    </dl>

    <dl class="row">
        <div class="form-group row">
            <label asp-for="Area" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(Model => Model.Area)
        </div>
    </dl>

    <dl class="row">
        <div class="form-group row">
            <label asp-for="Location" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(Model => Model.Location)
        </div>
    </dl>

    <dl class="row">
        <div class="form-group row">
            <label asp-for="RoomNumber" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(Model => Model.RoomNumber)
        </div>
    </dl>

    <dl class="row">
        <div class="form-group row">
            <label asp-for="BuildingType" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(Model => Model.BuildingType)
        </div>
    </dl>


    <dl class="row">
        <div class="form-group row">
            <label asp-for="CreateAt" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(Model => Model.CreateAt)
        </div>
    </dl>

    <dl class="row">
        <div class="form-group row">
            <label asp-for="ModifiedAt" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(Model => Model.ModifiedAt)
        </div>
    </dl>

    <form asp-action="DeleteConfirmation">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Delete" class="btn btn-danger" />
        <a asp-action="index" asp-controller="realestate" class="btn btn-primary">Back</a>
    </form>

</div>